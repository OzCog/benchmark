#! /usr/bin/env gnuplot
#
# Create a graph of the genes participating in pathways.
#
set term png
set out 'path-participants.png'

set logscale x
set logscale y
set format y "10^{%L}"
set title "Number paths that were participated in"
set xlabel "Gene Rank"
set ylabel "Number of triangles participated in"

# Eyeballed fit to the data.
plot "path-participants.csv" using 1:3 title "data", \
     "path-participants.csv" using 1:(10000 / ($1)**0.5) with lines title "1/sqrt(rank)"
